<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0/EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.falldowndetectionserver.mapper.TestMapper">
    <select id="select" resultType="testDTO">
        SELECT TEST_ID, TEST_NAME FROM TBL_TEST;
    </select>

    <insert id="insert">
        INSERT INTO TBL_TEST(TEST_ID, TEST_NAME) VALUES (#{testId}, #{testName});
    </insert>
</mapper>